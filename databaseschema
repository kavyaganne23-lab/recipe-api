CREATE TABLE "Recipes" (
    "id" SERIAL,
    "cuisine" VARCHAR(255),
    "title" VARCHAR(255) NOT NULL,
    "rating" FLOAT,
    "prep_time" INTEGER,
    "cook_time" INTEGER,
    "total_time" INTEGER,
    "description" TEXT,
    "nutrients" JSONB,
    "serves" VARCHAR(255),
    "createdAt" TIMESTAMP WITH TIME ZONE NOT NULL,
    "updatedAt" TIMESTAMP WITH TIME ZONE NOT NULL,
    PRIMARY KEY ("id")
);